{
  "name": "my-qwik-basic-starter",
  "description": "Demo App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "packageManager": "pnpm@8.7.5",
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr --port 9090",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "postinstall": "prisma generate",
    "preview": "qwik build preview && vite preview --open",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.2.14",
    "@builder.io/qwik-city": "^1.2.14",
    "@builder.io/qwik-react": "0.5.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.0",
    "@mui/x-data-grid": "^6.4.0",
    "@prisma/client": "5.5.2",
    "@types/bootstrap": "5",
    "@types/eslint": "^8.44.4",
    "@types/node": "^20.8.4",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "autoprefixer": "^10.4.14",
    "daisyui": "^3.9.4",
    "eslint": "^8.51.0",
    "eslint-plugin-qwik": "^1.2.14",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "prisma": "^5.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.69.5",
    "tailwindcss": "3.3.3",
    "tailwindcss-animatecss": "^3.0.4",
    "typescript": "^5.2.2",
    "undici": "^5.26.0",
    "vite": "^4.4.11",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@ant-design/pro-components": "^2.6.34",
    "@builder.io/qwik-react": "0.5.0",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@modular-forms/qwik": "^0.21.0",
    "@mui/material": "5.11.9",
    "@mui/x-data-grid": "5.17.24",
    "@unpic/qwik": "^0.0.29",
    "antd": "^5.10.2",
    "bootstrap": "5",
    "flowbite": "^2.0.0",
    "qwik-image": "^0.0.8",
    "valibot": "^0.19.0"
  }
}
